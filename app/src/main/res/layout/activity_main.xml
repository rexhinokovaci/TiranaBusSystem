<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <ImageView
        android:id="@+id/zonatVeriore"
        android:layout_width="150dp"
        android:layout_height="120dp"

        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="30dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="82dp"
        android:adjustViewBounds="true"
        android:src="@drawable/zonatveriore" />

    <ImageView
        android:id="@+id/zonatJugore"
        android:layout_width="150dp"
        android:layout_height="120dp"

        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="82dp"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="20dp"
        android:adjustViewBounds="true"
        android:src="@drawable/zonatjugore" />

    <ImageView
        android:id="@+id/urbaniTirane"
        android:layout_width="150dp"
        android:layout_height="120dp"

        android:layout_below="@+id/zonatVeriore"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"

        android:layout_marginLeft="30dp"
        android:layout_marginTop="82dp"
        android:adjustViewBounds="true"
        android:src="@drawable/urbanitirane" />


    <ImageView
        android:id="@+id/taksiteTirane"
        android:layout_width="150dp"
        android:layout_height="120dp"

        android:layout_below="@+id/zonatJugore"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="82dp"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="36dp"
        android:adjustViewBounds="true"
        android:src="@drawable/taksitenetirane" />


    <ImageView
        android:id="@+id/oraretTirane"
        android:layout_width="150dp"
        android:layout_height="120dp"
        android:layout_below="@+id/urbaniTirane"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="30dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="82dp"
        android:adjustViewBounds="true"
        android:src="@drawable/oraretetiranes" />


    <ImageView
        android:id="@+id/oraretRetheve"
        android:layout_width="150dp"
        android:layout_height="120dp"

        android:layout_below="@+id/taksiteTirane"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="82dp"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"
        android:adjustViewBounds="true"

        android:src="@drawable/oraretrretheve" />


</RelativeLayout>




